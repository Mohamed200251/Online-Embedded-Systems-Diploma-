#include <stdio.h>
int main()
{
	float a, b, temp;
	printf("Enter the value of a: ");
	fflush(stdin); fflush(stdout);
	scanf("%f",&a);
	printf("Enter the value of b: ");
	fflush(stdin); fflush(stdout);
	scanf("%f",&b);
	temp=a;
	a=b; b=temp;
	printf("\nAfter swapping, value of a = %f", a);
	printf("\nAfter swapping, value of b = %f", b);
}

